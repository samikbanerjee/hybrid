<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- 	using class tag and priority annotation -->
<suite name="LoginPageTest" parallel="tests">
<parameter name="appURL" value="https://50.193.56.105:9002/yacceleratorstorefront/electronics/en/?site=electronics"/>
 
  <!-- make sure to use @Beforetest annotation on initializeTestBaseSetup method in BaseSetup class -->
  <test name="LoginTestInChrome">
  <parameter name="browserType" value="chrome"/>
    <classes>
      <class name="com.pragiti.test.LoginPageTest"/>
    </classes>
  </test>
  

  <test name="LoginTestInIE">
  <parameter name="browserType" value="IE"/>
    <classes>
      <class name="com.pragiti.test.LoginPageTest"/>
    </classes>
  </test>
  
  <test name="LoginTestInFF">
  <parameter name="browserType" value="firefox"/>
    <classes>
      <class name="com.pragiti.test.LoginPageTest"/>
    </classes>
  </test>
  
</suite>