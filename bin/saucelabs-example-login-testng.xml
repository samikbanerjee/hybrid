<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- 	using class tag and priority annotation : sequnecing -->
<suite name="LoginPageTest">
<parameter name="appURL" value="https://50.193.56.105:9002/yacceleratorstorefront/electronics/en/?site=electronics"/>
<parameter name="browserType" value="saucelabs"/>
  <test name="LoginTest">
    <classes>
      <class name="com.pragiti.test.LoginPageTest"/>
<!-- here you can add other cases to run test together for ex.
	  <class name="com.pragiti.test.MyAccountPageTest"/>
      <class name="com.pragiti.test.WishlistTest"/> -->
    </classes>
  </test>
</suite>

